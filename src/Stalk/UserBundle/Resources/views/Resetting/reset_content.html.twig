{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_request">
    
    <img src="{{ asset('bundles/stalkaffiliate/images/company_logo.png') }}" alt="" class="logo">
    
    <p class="title">Партнерская программа</p>

    {{ form_row(form.plainPassword.first, { 'attr': {'class': 'form-control'} }) }}
    
    {{ form_row(form.plainPassword.second, { 'attr': {'class': 'form-control'} }) }}
    
    {{ form_rest(form) }}
    
    <br/>
    <div>
        <input type="submit" value="{{ 'resetting.reset.submit'|trans }}" class="btn btn-lg btn-primary btn-block"/>
    </div>
</form>
